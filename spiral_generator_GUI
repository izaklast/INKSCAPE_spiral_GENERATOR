  <param name="radio1" type="optiongroup" _gui-text="Number of copies: ">
    <option value="Rows:">Rows</option>
    #needs int input
    <option value="Columns">columns/option>
    #needs int input
    <option value="Symmetrical">Make symmetrical both ways</option>
      #yes or no radio button
  </param>
  <param name="radio2" type="optiongroup" _gui-text="Select second option: ">
    <option value="string11">string1</option>
    <option value="string22">string2</option>
    <option>test3!</option>
  </param>
  <effect>
    <object-type>all</object-type>
    <effects-menu>
      <submenu _name="Developer Examples"/>
    </effects-menu>
  </effect>
